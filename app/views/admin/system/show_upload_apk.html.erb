<div class="bs-example" id="category_res"> 
<div class="row app-list">
	<div class="col-lg-2"></div>
	<div class="col-lg-8">
		<div class="panel panel-rank">
			<div class="panel-heading">
			    <div>添加APK</div>
			</div> 
			
			<div class="panel-action">
			    <div class="navbar">
					<span><%= link_to "返回", admin_system_index_path, :class => 'navbar-link'%></span>
				</div>
			</div> 
			          
			<div class="row app-list">
			    <%= form_for(@app, :url => admin_system_upload_apk_path, :html => {:class => "form-horizontal"}) do |f| %>
					<div class="form-group">
						<label for="name" class="col-lg-2 control-label">名称</label>
						<div class="col-lg-10">
							<%= f.text_field :name, :autofocus => true,  :class => "form-control", :placeholder => "名称" %>
						</div>
					</div>	
					<div class="form-group">
						<label for="apk" class="col-lg-2 control-label">APK文件</label>
						<div class="col-lg-10">
							<%= f.file_field :apk,  :class => "form-control" %>
							<%= f.submit "上传", :class => "btn btn-primary", :id => "upload_apk"%>
						</div>
					</div>
				<% end %>
			</div> 
		</div>
	</div>
	<div class="col-lg-2"></div>
</div>
</div>